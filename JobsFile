#!/usr/bin/env groovy

pipelineJob('PipelineFromDsl') {
	def repo = "https://github.com/robrodi/JenkinsPipelineGo.git"
  	
    triggers {
		scm('H/5 * * * *')
  	}
    description("Pipeline for $repo")
    scm {
	    github('robrodi/JenkinsPipelineGo')
    }
    steps{
          shell('echo START')

	  groovyScriptFile('JenkinsFile')  
    }
    
}
